# AWS Configuration for Elf Name Generator
# Copy this file to .env and fill in your values

# Option 1: Use AWS Profile (recommended for temporary credentials, SSO, MFA)
# AWS_PROFILE=your-profile-name

# Option 2: Use static credentials (not recommended for production)
# AWS_ACCESS_KEY_ID=your_access_key_id
# AWS_SECRET_ACCESS_KEY=your_secret_access_key
# AWS_SESSION_TOKEN=your_session_token  # Only needed for temporary credentials

# AWS Region (defaults to us-east-2 if not set)
AWS_DEFAULT_REGION=us-east-2
